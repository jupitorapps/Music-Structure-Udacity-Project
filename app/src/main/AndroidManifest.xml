<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.first.project4musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".hindi_song_ringtone"
            android:label="@string/hindiSongRingtone" />
        <activity
            android:name=".djmix"
            android:label="@string/djMixRingtone" />
        <activity
            android:name=".animalSound"
            android:label="@string/animalsSound" />
        <activity
            android:name=".otherSounds"
            android:label="@string/otherSounds" />
        <activity android:name=".PaymentActivity"></activity>
    </application>

</manifest>